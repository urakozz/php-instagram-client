{
	"name": "kozz/instagram-client",
	"description": "Instatram Client",
	"keywords": ["instagram", "client"],
	"license": "MIT",
	"type": "project",
	"require": {
		"jms/serializer":"~0.16",
    "doctrine/common": "~2.5",
		"guzzlehttp/guzzle":"~6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"mockery/mockery":"~0.9.0",
		"codeclimate/php-test-reporter": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Instagram\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Instagram\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
	"config": {
		"preferred-install": "dist"
	}
}
